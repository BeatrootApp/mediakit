module Mediakit
  module Codecs
    module <%= @item.type.to_s.classify %>
      class Codec<%= @item.name.classify %>
        def name
          '<%= @item.name %>'
        end

        def encoders
          <%= @item.encoders.inspect %>
        end

        def decoders
          <%= @item.decoders.inspect %>
        end
      end
    end
  end
end
